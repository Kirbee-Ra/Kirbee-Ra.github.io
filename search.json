---
layout: null
permalink: /search.json
---
[
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title | escape }}",
      "tags"     : "{{ post.tags | join: ', ' }}",
      "url"      : "{{ post.url | relative_url }}",
      "content"  : "{{ post.content | strip_html | strip_newlines | jsonify | remove_first: '"' | remove_last: '"' | escape }}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
